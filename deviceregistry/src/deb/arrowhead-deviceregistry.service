[Unit]
Description=arrowhead-deviceregistry
After=network.target mysql.target arrowhead-serviceregistry-sql.service
Requires=arrowhead-serviceregistry-sql.service arrowhead-authorization.service

[Service]
WorkingDirectory=/etc/arrowhead/systems/deviceregistry
ExecStart=/usr/bin/java -jar /usr/share/arrowhead/arrowhead-deviceregistry-4.1.1-SNAPSHOT.jar -d -daemon -tls
TimeoutStopSec=5
Type=simple
User=arrowhead
Group=arrowhead

[Install]
WantedBy=default.target
